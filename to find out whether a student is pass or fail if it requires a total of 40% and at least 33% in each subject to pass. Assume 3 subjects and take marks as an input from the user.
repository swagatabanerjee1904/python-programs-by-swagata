sub1=int(input("Enter your marks for sub1:"))
sub2=int(input("Enter your marks for sub2:"))
sub3=int(input("Enter your marks for sub3:"))   
if(sub1<33 or sub2<33 or sub3<33):
    print("You are fail")
elif((sub1+sub2+sub3)/3)<40:
    print("You are fail")
else:
    print("Congratulations")        
